{
  "name": "dodekeract.com",
  "version": "0.0.0",
  "description": "https://dodekeract.com",
  "main": "build/server/",
  "scripts": {
    "build": "npm run clear --silent && npm run copy --silent && npm run transpile-javascript --silent && npm run bundle --silent && npm run transpile-style --silent",
    "bundle": "webpack --progress --colors build/client/index.js build/client/bundle.js --optimize-minimize --optimize-dedupe",
    "clear": "rm -rf build/",
    "copy": "mkdir -p ./build/client && cp source/client/index.html build/client/index.html",
    "start": "node build/server/",
    "transpile-javascript": "babel --presets react,es2015,stage-0 --plugins transform-async-to-generator source -d build",
    "transpile-style": "node-sass source/client/styles/main.sass build/client/style.css",
    "watch": "nodemon --watch source/ --watch package.json -e js,json,html,css,sass,scss,png,jpg,jsx --exec \"npm run build --silent && npm run start --silent\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodekeract/com.git"
  },
  "keywords": [
    "dodekeract"
  ],
  "author": "Florian Wendelborn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dodekeract/com/issues"
  },
  "homepage": "https://github.com/dodekeract/com#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "koa": "^1.2.4",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2"
  }
}
