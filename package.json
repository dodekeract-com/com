{
  "name": "dodekeract.com",
  "version": "0.0.0",
  "description": "https://dodekeract.com",
  "main": "build/server/",
  "scripts": {
    "build": "yarn run clear -s && yarn run transpile-javascript -s && yarn run bundle -s && yarn run transpile-style -s",
    "bundle": "webpack build/client/index.js build/client/main.js",
    "clear": "rm -rf build/",
    "start": "node build/server/",
    "transpile-javascript": "babel --presets react,es2015,stage-0 source -d build",
    "transpile-style": "node-sass source/client/styles/_import.sass build/client/main.css",
    "watch": "nodemon --watch source/ --watch package.json -e js,json,html,css,sass,scss,png,jpg,jsx --exec \"yarn run build -s && yarn run start -s\"",
    "docker-build": "docker build -t dodekeract/com .",
    "docker-start": "docker rm -f dodekeract.com; docker run -p 45000:8080 -d --name dodekeract.com dodekeract/com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodekeract/com.git"
  },
  "keywords": [
    "dodekeract"
  ],
  "author": "Florian Wendelborn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dodekeract/com/issues"
  },
  "homepage": "https://github.com/dodekeract/com#readme",
  "dependencies": {
    "hyperapp": "^0.7.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^4.0.0-alpha.6",
    "robogen": "^0.0.2",
    "spirit": "^0.4.0",
    "spirit-error": "^0.0.5",
    "spirit-router": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2"
  }
}
